<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE note [
<!ENTITY energy "7">
<!ENTITY prod "P10ih">
<!ENTITY trig_sufix "">
]>
<job simulateSubmission ="false" maxFilesPerProcess="15"  fileListSyntax="xrootd">
  
  <command>
      starver dev
      root4star -l -b -q readMuDst.C\(\"$FILELIST\",\"auau_$JOBID.root\",&energy;\)
 </command>

  
  <SandBox installer="ZIP">
    <Package name="RunSet2">
      <File>file:./.sl73_gcc485</File>
      <File>file:./readMuDst.C</File>

    </Package>
  </SandBox>

<input URL="catalog:star.bnl.gov?production=&prod;,trgsetupname=AuAu&energy;_production&trig_sufix;,storage=HPSS,filetype=daq_reco_MuDst,filename~st_physics,sanity=1" nFiles="all" />
  <stderr URL="file:/gpfs01/star/pwg/dneff/scratch/log/&energy;GeV/err_&energy;GeV$JOBID.err"/>
  <stdout URL="file:/gpfs01/star/pwg/dneff/scratch/log/&energy;GeV/log_&energy;GeV$JOBID.out" />

    <output fromScratch="*.root" toURL="file:/gpfs01/star/pwg/dneff/scratch/output/&energy;GeV/" />
    <output fromScratch="*.log" toURL="file:/gpfs01/star/pwg/dneff/scratch/log/" />
 
  <Generator>
    <Location>/gpfs01/star/pwg/dneff/scratch/log</Location>
    <ScriptLocation>/gpfs01/star/pwg/dneff/scratch/script/7GeV/</ScriptLocation>
    <ListLocation>/gpfs01/star/pwg/dneff/scratch/list/7GeV/</ListLocation>
  </Generator>
  
</job>
